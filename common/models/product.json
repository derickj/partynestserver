{
  "name": "Product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "default": " "
    },
    "description": {
      "type": "string",
      "required": true,
      "default": " "
    },
    "unitprice": {
      "type": "number",
      "default": 0
    },
    "label": {
      "type": "string",
      "default": " "
    },
    "image": {
      "type": "string",
      "required": true,
      "default": "undefined.jpg"
    }
  },
  "validations": [],
  "relations": {
    "reviews": {
      "type": "hasMany",
      "model": "Review",
      "foreignKey": ""
    },
    "wishlist": {
      "type": "hasMany",
      "model": "Wishlist",
      "foreignKey": ""
    },
    "featured": {
      "type": "hasMany",
      "model": "Featured",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
